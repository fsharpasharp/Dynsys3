Clear[x, y, r]
lhs = {{x/r, y/r}, {-y/r^2, x/r^2}}
inv = Inverse[lhs]
rhs = {{r (\[Mu] - r^2)}, {\[Omega] + \[Nu] r^2}}
sol = Simplify[inv . rhs /. {r -> Sqrt[x^2+y^2], r^2 -> x^2 + y^2}]
xdot[x_, y_] = sol[[1,1]]
ydot[x_, y_] = sol[[2,1]]
\[Mu] = 1
\[Nu] = 1
\[Omega] = 1 
pic = StreamPlot[{xdot[x, y], ydot[x, y]}, {x, -2, 2}, {y, -2, 2}]
Export["streamplot1b.png", pic]
